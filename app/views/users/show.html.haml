.wrapper
  = render 'shared/header'

  .user-main
    .user-main__contents
      .left-contents
        = render 'shared/side-bar'

      .right-contents
        .right-contents__user
          = link_to root_path do
            .right-contents__user__icon
              .right-contents__user__icon__figure
              .right-contents__user__icon__user-name
                -# =@user.nickname

  = render 'shared/footer'